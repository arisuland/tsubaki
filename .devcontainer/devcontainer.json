{
  "name": "Tsubaki",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "workspace",
  "workspaceFolder": "/workspace",
  "extensions": [
    "golang.go"
  ],
  "forwardPorts": [
    80,    // http
    5432,  // postgres
    2181,  // zookeeper
    9092,  // kafka
    6379,  // redis
    17903, // backend
    29092  // kafka
  ]
}
